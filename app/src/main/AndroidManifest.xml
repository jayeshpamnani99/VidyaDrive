<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.harshit.vidyadrive">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/somaiyavidyavihar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/somaiyavidyavihar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubjectsActivity" />
        <activity android:name=".ChaptersActivity" />
        <activity android:name=".UploadsActivity" />
        <activity android:name=".pdfActivity" />
        <activity
            android:name=".YearActivity"
            android:label="@string/title_activity_year"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CourseActivity"></activity>
    </application>

</manifest>